#: src/spi:182
msgid "Available packages:"
msgstr "Διαθέσιμα πακέτα:"

#: src/spi:185 src/spi:197
msgid "None"
msgstr "Κανένα"

#: src/spi:190 src/spi:202 src/spi:361
msgid "Installed"
msgstr "Εγκατεστημένο"

#: src/spi:192 src/spi:204 src/spi:363
msgid "Not installed"
msgstr "Μη εγκατεστημένο"

#: src/spi:194
msgid "Available SlackBuilds:"
msgstr "Διαθέσιμα SlackBuilds:"

#: src/spi:223 src/spi:267 src/spi:367
msgid "No such package or SlackBuild:"
msgstr "Δεν υπάρχει πακέτο ή SlackBuild με αυτό το όνομα:"

#: src/spi:235
msgid "The following packages will be installed:"
msgstr "Τα ακόλουθα πακέτα θα εγκατασταθούν:"

#: src/spi:241
msgid "The following SlackBuilds will be installed:"
msgstr "Τα ακόλουθα SlackBuilds θα εγκατασταθούν:"

#: src/spi:245
msgid "Nothing to install"
msgstr "Τίποτα προς εγκατάσταση"

#: src/spi:302
msgid "Updating package cache..."
msgstr "Ενημέρωση της τοπικής μνήμης πακέτων..."

#: src/spi:306
msgid "Updating SlackBuild cache..."
msgstr "Ενημέρωση της τοπικής μνήμης SlackBuild..."

#: src/spi:420
msgid "USAGE:"
msgstr "ΧΡΗΣΗ:"

#: src/spi:420
msgid "[OPTIONS] [STRING(s)]"
msgstr "[ΕΠΙΛΟΓΕΣ] [ΛΕΞΗ/ΛΕΞΕΙΣ]"

#: src/spi:422
msgid "OPTIONS:"
msgstr "ΕΠΙΛΟΓΕΣ:"

#: src/spi:423
msgid "search for specified packages (default action)"
msgstr "αναζήτηση πακέτων (εξ ορισμού επιλογή)"

#: src/spi:424
msgid "show details about package or SlackBuild"
msgstr "εμφάνιση λεπτομερειών για το πακέτο ή το SlackBuild"

#: src/spi:425
msgid "purge package and SlackBuild caches"
msgstr "εκκαθάριση των τοπικών αρχείων πακέτων και SlackBuild"

#: src/spi:426
msgid "update package and SlackBuild caches"
msgstr "ενημέρωση της τοπικής μνήμης πακέτων και SlackBuild"

#: src/spi:427
msgid "upgrade packages"
msgstr "αναβάθμιση πακέτων"

#: src/spi:428
msgid "install specified packages"
msgstr "εγκατάσταση πακέτων"

#: src/spi:429
msgid "simulate installation of specified packages"
msgstr "προβολή πακέτων που πρόκειται να εγκατασταθούν"

#: src/spi:430
msgid "disable colour output"
msgstr "απενεργοποίηση έγχρωμης εξόδου"

#: src/spi:431
msgid "this help message"
msgstr "το παρόν μήνυμα βοήθειας"
